map <Space>E :action ReSharperGotoPrevErrorInSolution<CR>
map <Space>s :action SurroundWith<CR>
map <Space>l :action InsertLiveTemplate<CR>
map <Space>u :action FindUsages<CR>
map <Space>d :action EditorDuplicate<CR>
map <Space>v :action IntroduceVariable<CR>
map <Space>m :action ExtractMethod<CR>
map <Space>o :action Move<CR>
map <Space>t :action GotoClass<CR>
map <Space>sd :action ReSharperSelectContainingDeclaration<CR>
map <Space>tr :action Rider.UnitTesting.RunContext<CR>
map <Space>td :action RiderUnitTestDebugContextAction<CR>
map <Space>ta :action Rider.UnitTesting.RunSolution<CR>
map <Space>tt :action ActivateUnitTestsToolWindow<CR>
